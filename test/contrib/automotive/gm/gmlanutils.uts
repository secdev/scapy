            ecusimSuccessfullyExecuted = False
            nr = GMLAN() / GMLAN_NR(requestServiceId=0x27, returnCode=0x35)
            isotpsock2.send(nr)
        else:
            pr = GMLAN()/GMLAN_SAPR(subfunction=2)
            isotpsock2.send(pr)

