        nr = GMLAN()/GMLAN_NR(requestServiceId=0x34, returnCode=0x22)
        isotpsock2.send(nr)

thread = threading.Thread(target=ecusim)
thread.start()
time.sleep(0.05)
with ISOTPSocket(new_can_socket(iface0), sid=0x242, did=0x642, basecls=GMLAN) as isotpsock:
