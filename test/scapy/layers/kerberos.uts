= RFC4556 Test Vectors - octetstring2key - Utils

from scapy.libs.rfc3961 import EncryptionType, octetstring2key

def _strip(x):
    return bytes.fromhex(x.replace(" ", "").replace("\n", ""))

def _k_truncate_output(etype, input):
    with mock.patch('scapy.libs.rfc3961.Key.random_to_key', side_effect=Bunch):
        result = octetstring2key(EncryptionType.AES256_CTS_HMAC_SHA1_96, INPUT)
    return result.seed

