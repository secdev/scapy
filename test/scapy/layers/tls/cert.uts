= PrivKeyECDSA sign & verify
~ crypto_advanced
a = PrivKeyECDSA()
a.fill_and_store()
msg = b"Scapy test message"
data = a.sign(msg)
assert a.verify(msg, data)
assert not a.verify(b"Hello", data)

= PubKeyECDSA verify
~ crypto_advanced
assert isinstance(a.pubkey, PubKeyECDSA)

b = PubKeyECDSA(cryptography_obj=a.pubkey.pubkey)
assert b.verify(msg, data)
assert not b.verify(b"Hello", data)

