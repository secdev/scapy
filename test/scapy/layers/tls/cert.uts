assert certReqInfo.attributes[3].type.oidname == "OID_ENROLLMENT_CSP_PROVIDER"
assert certReqInfo.attributes[3].values[0].value.ProviderName.val.decode("utf-16be") == 'Microsoft Platform Crypto Provider'

assert certReqInfo.attributes[4].type.oidname == "extensionRequest"
exts = certReqInfo.attributes[4].values[0].value.extensions

assert exts[0].extnID.oidname == "OID_CERTIFICATE_TEMPLATE"
