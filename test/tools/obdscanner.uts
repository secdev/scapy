                               b"3: 'A\\x03\\x00\\x02'",
                               b"11: 'A\\x0bd'",
                               b"15: 'A\\x0fZ'"]
        else:
            expected_output = [b"Service 3:\nb'C\\x01\\x00\\x00'",
                               b"Service 1:\n{",
                               b"3: b'A\\x03\\x00\\x02'",
                               b"11: b'A\\x0bd'",
                               b"15: b'A\\x0fZ'"]

        for out in expected_output:
            assert bytes_encode(out) in bytes_encode(std_out)
    finally:
        isocan.send(CAN(identifier=0x7e0, data=b'\x07\xff\xff\xff\xff\xff\xff\xff'))
